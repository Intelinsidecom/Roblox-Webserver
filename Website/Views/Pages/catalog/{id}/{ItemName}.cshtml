            <div class="content item-page-main">
                <div id=item-container class=page-content data-item-id="@Model.Id" data-item-name="@Model.Name" data-asset-type="Package" data-userasset-id="" data-is-purchase-enabled=true data-product-id="@Model.Id" data-bc-requirement=0 data-expected-currency="1" data-expected-price="@(Model.PriceRobux ?? 0)" data-seller-name="@Model.CreatorName" data-expected-seller-id="@Model.CreatorId" data-lowest-private-sale-userasset-id="" data-is-limited-unique="@Model.IsLimitedUnique" data-user-id="@User?.FindFirst(System.Security.Claims.ClaimTypes.NameIdentifier)?.Value" data-asset-granted=False data-forward-url="" data-avatar-wear-url="https://avatar.roblox.com/v1/avatar/assets/@Model.Id/wear" data-avatar-remove-url="https://avatar.roblox.com/v1/avatar/assets/@Model.Id/remove" data-current-time="@DateTime.UtcNow">
                    <div class=system-feedback>
                        <div class=alert-system-feedback>
                            <div class="alert alert-success">Purchase Completed</div>
                        </div>
                        <div class=alert-system-feedback>
                            <div class="alert alert-warning">Error occurred</div>
                        </div>
                    </div>
                    <div class="section-content top-section">
                        <div class=item-name-container>
                            <h2>@Model.Name</h2>
                            <div>
                                <span class=text-label>By <a href="/users/@Model.CreatorId/profile" class=text-name>@Model.CreatorName</a></span>
                                @if (Model.IsOwned)
                                {
                                    <div class=divider>&nbsp;</div>
                                    <div class=label-checkmark><span class=icon-checkmark-white-bold></span></div><span>Item Owned</span>
                                }

                                @* <div id=phone-sale-clock class="sale-clock phone-sale-clock" data-off-sale-deadline="10/31/2017 5:50:49 PM"><span class=icon-red-timer-16x16></span>
                                    <div class=text></div>
                                </div> *@
                            </div>
                        </div>
                        <div class=item-thumbnail-container>
                            <div id=AssetThumbnail class="thumbnail-holder thumbnail-Large three-dee-static" data-reset-enabled-every-page data-3d-thumbs-enabled three-dee-static data-3dtype=static data-url="/thumbnail/asset?assetId=@Model.Id&amp;thumbnailFormatId=254&amp;width=420&amp;height=420"><span class=thumbnail-span data-3d-url="/asset-thumbnail-3d/json?assetId=@Model.Id"><img src="@Model.ImageUrl"></span>

                                <div class=equipped-marker></div><span class="enable-three-dee three-dee-static-icon"></span>
                                @* <div class="asset-status-icon status-sale has-icon"><span class=icon-clock></span> <span> </span></div> *@
                            </div>
                            <script>
                                (function() {
                                    if (Roblox && Roblox.Performance) {
                                        Roblox.Performance.setPerformanceMark("itemReskin_thumbnail_loaded");
                                    }
                                })();
                            </script>
                        </div>
                        <div id=item-details class="content-overflow-toggle content-height content-overflow-page-loading item-details">
                            <div class="clearfix price-container">
                                <div class=price-container-text>
                                    @if (Model.IsOwned)
                                    {
                                        <div class=item-first-line>This item is available in your inventory.</div>
                                    }
                                    <div class="text-label field-label price-label">Price</div>
                                    <div class=price-info>
                                        <div class="icon-text-wrapper clearfix icon-robux-price-container">
                                            <span class=icon-robux></span>
                                            <span class=text-robux-lg>
                                                @if (Model.PriceRobux.HasValue)
                                                {
                                                    @Model.PriceRobux.Value.ToString("N0")
                                                }
                                                @* else
                                                {
                                                    @:Offsale
                                                } *@
                                            </span>
                                        </div>
                                    </div>

                                </div>
                                <div class=action-button>
                                    @if (Model.IsOwned)
                                    {
                                        <a id=edit-avatar-button href="https://web.roblox.com/my/avatar" class="btn-fixed-width-lg btn-control-md" data-button-action=avatar> Edit Avatar </a>
                                    }
                                    else
                                    {
                                        <button type=button class="btn-fixed-width-lg btn-primary-lg PurchaseButton" data-button-type=main data-button-action=buy data-se=item-buyforrobux data-item-name="@Model.Name" data-seller-name="@Model.CreatorName" data-item-id="@Model.Id" data-expected-price="@(Model.PriceRobux ?? 0)" data-expected-currency="1" data-asset-type="Package" data-expected-seller-id="@Model.CreatorId"> Buy </button>
                                    }
                                    @* <div id=sale-clock class="sale-clock desktop-sale-clock" data-off-sale-deadline="10/31/2017 5:50:49 PM">
                                        <div class=text></div>
                                    </div> *@
                                </div>
                            </div>
                            <div class="clearfix item-mobile-description item-field-container">
                                <pre class=description-content>@Model.Description</pre>

                            </div>
                            <div class="clearfix item-type-field-container">
                                <div class="text-label field-label">Type</div><span class=field-content>@Model.AssetTypeLabel</span>

                            </div>
                            <div class="clearfix item-field-container">
                                <div class="text-label field-label">Genres</div>
                                <div class=field-content><span class="text-name item-genre">@Model.GenreLabel</span></div>

                            </div>
                            <div class="clearfix toggle-target item-field-container">
                                <div class="text-label field-label">Description</div>
                                <pre id=item-details-description class="content-overflow-toggle content-height content-overflow-page-loading field-content description-content">@Model.Description<span class="hidden toggle-content" data-container-id=item-details-description>Read More</span></pre>

                            </div>
                            <div class="hide show-more-end" data-container-id=item-details></div><button type=button class="hidden btn-full-width btn-control-md toggle-content" data-container-id=item-details>Read More</button>
                        </div>
                        <ul class="item-social-container clearfix include-favorite">
                            <li class=favorite-button-container>
                                <div class=tooltip-container data-toggle=tooltip data-original-title="Add to Favorites"><a id=toggle-favorite data-toggle-url=/favorite/toggle data-assetid=@Model.Id data-isguest=False data-signin-url="https://web.roblox.com/newlogin?returnUrl=%2Fcatalog%2F@Model.Id%2F@Model.Name"> <span class="text-favorite favoriteCount" id=result>@((Model.FavoritedCount ?? 0).ToString("N0"))</span>
                                        <div id=favorite-icon class="icon-favorite@(Model.IsFavorited ? " favorited" : string.Empty)"></div>
                                    </a></div>
                        </ul>
                    </div>
                    <div ng-modules="robloxApp, recommendations">
                        <script>
                            var Roblox = Roblox || {};
                            Roblox.InventoryData = {
                                "inventoryDomain": "https://inventory.roblox.com",
                                "useInventorySite": true
                            };
                        </script>
                        <div class=current-items ng-controller=recommendationsController ng-init="initializeWithModelValues(@Model.Id, 32, 7)" ng-show="recommendationsData.items.length > 0" ng-cloak>
                            <div class="container-list recommendations-container">
                                <div class="container-header recommendations-header">
                                    <h3>Recommended {{ recommendationsData.assetTypeName | capitalize }}</h3>
                                </div>
                                <div class=recommended-items-slider>
                                    <ul class="hlist item-cards recommended-items" ng-class="{'item-cards-embed' : recommendationsLayout.numberOfItemsToDisplay &lt; 7}">
                                        <li ng-repeat="item in recommendationsData.items" class="list-item item-card recommended-item">
                                            <div class="item-card-container recommended-item-link"><a ng-href={{item.Item.AbsoluteUrl}} class=item-card-link>
                                                    <div class="item-card-thumb-container recommended-thumb"><img ng-src={{item.Thumbnail.Url}} thumbnail=item.Thumbnail image-retry class=item-card-thumb> <span ng-show=item.AssetRestrictionIcon ng-class="'icon-' + item.AssetRestrictionIcon.CssTag + '-label'"> </span></div>
                                                    <div class="text-overflow item-card-name recommended-name" title="{{ item.Item.Name }}">{{ item.Item.Name }}</div>
                                                </a>
                                                <div ng-if=item.Item.AudioUrl class=MediaPlayerControls>
                                                    <div class="MediaPlayerIcon icon-play" data-mediathumb-url={{item.Item.AudioUrl}} data-jplayer-version=2.9.2></div>
                                                </div>
                                                <div class="text-overflow item-card-creator recommended-creator"><span class="xsmall text-label recommended-creator-by">By</span> <a class="xsmall text-overflow text-link" ng-href={{item.Creator.CreatorProfileLink}}>{{ item.Creator.Name }}</a></div>
                                                <div class="text-overflow item-card-price"><span class=icon-robux-16x16 ng-show=item.HasPrice></span> <span class=text-robux ng-show=item.HasPrice>{{ item.Item.Price | abbreviate : 1 }}</span> <span class=text-label ng-hide=item.HasPrice> <span ng-if="item.Product.NoPriceText.length > 0" ng-class="{'text-robux': item.Product.NoPriceText === 'Free'}"> {{item.Product.NoPriceText}} </span> </span></div>
                                            </div>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
