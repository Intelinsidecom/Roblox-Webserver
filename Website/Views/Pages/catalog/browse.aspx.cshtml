<script type="text/javascript">
    $(function () {
        // Fallback for category/subcategory clicks when Roblox.Pages.Catalog is not initialized
        $("a.assetTypeFilter[data-category][data-types]").on("click.catalogFallback", function (e) {
            if (window.Roblox && Roblox.Pages && Roblox.Pages.Catalog && Roblox.Pages.Catalog.pagestate) {
                return;
            }

            e.preventDefault();

            var $link = $(this);
            var category = $link.data("category");
            var subcategory = $link.data("types");

            if (category === undefined || subcategory === undefined) {
                return;
            }

            var url = "/catalog/browse.aspx?Category=" + encodeURIComponent(category) + "&Subcategory=" + encodeURIComponent(subcategory);
            window.location = url;
        });

        // Fallback for keyword search when Roblox.Pages.Catalog is not initialized
        function performFallbackSearch() {
            if (window.Roblox && Roblox.Pages && Roblox.Pages.Catalog && Roblox.Pages.Catalog.pagestate) {
                return true;
            }

            var keyword = $.trim($("#keywordTextbox").val() || "");
            var category = $("#categoriesForKeyword").val() || "0";

            // Allow empty search, controller already handles Keyword precedence
            var url = "/catalog/browse.aspx?Category=" + encodeURIComponent(category);
            if (keyword.length > 0) {
                url += "&Keyword=" + encodeURIComponent(keyword);
            }

            window.location = url;
            return false;
        }

        $("#submitSearchButton").on("click.catalogFallback", function (e) {
            if (!performFallbackSearch()) {
                e.preventDefault();
            }
        });

        $("#keywordTextbox").on("keypress.catalogFallback", function (e) {
            if (e.which === 13) {
                if (!performFallbackSearch()) {
                    e.preventDefault();
                }
            }
        });

        // Force genre checkbox filters to use the MVC browse action instead of legacy AJAX
        var currentCategory = @((int)(ViewBag.Category ?? 0));
        var currentSubcategory = @((int)(ViewBag.Subcategory ?? 0));

        $("input.genreFilter").on("click.catalogGenreFallback", function (e) {
            var selected = $("input.genreFilter:checked").map(function () {
                return $(this).data("genreid");
            }).get();

            var url = "/catalog/browse.aspx?Category=" + encodeURIComponent(currentCategory)
                + "&Subcategory=" + encodeURIComponent(currentSubcategory);

            for (var i = 0; i < selected.length; i++) {
                url += "&Genres=" + encodeURIComponent(selected[i]);
            }

            e.preventDefault();
            window.location = url;
        });
    });
</script>

