@{ Layout = "_Layout"; }
@{
    var viewParam = Context.Request.Query["view"].ToString();
    if (string.IsNullOrEmpty(viewParam))
    {
        viewParam = Context.Request.Query["View"].ToString();
    }

    // Default to T-Shirts tab when no view is specified
    if (string.IsNullOrEmpty(viewParam))
    {
        viewParam = "2";
    }

    var viewName = "Games";
    if (!string.IsNullOrEmpty(viewParam))
    {
        switch (viewParam)
        {
            case "9":
                viewName = "Places";
                break;
            case "10":
                viewName = "Models";
                break;
            case "13":
                viewName = "Decals";
                break;
            case "21":
                viewName = "Badges";
                break;
            case "3":
                viewName = "Audio";
                break;
            case "24":
                viewName = "Animations";
                break;
            case "40":
                viewName = "Meshes";
                break;
            case "11":
                viewName = "Shirts";
                break;
            case "2":
                viewName = "T-Shirts";
                break;
            case "12":
                viewName = "Pants";
                break;
            case "38":
                viewName = "Plugins";
                break;
            default:
                viewName = "Games";
                break;
        }
    }
}
