<script>
    // Auto-load games list for My Creations tab and remove initial separator
    // Also probe group games endpoint and hide Group Creations tab if it 404s
    $(function () {
        // Ensure the gray separator is removed/hid immediately
        $('#MyCreationsTab .separator').remove();

        function showEmptyMessage($container) {
            $container.html('<div class="no-games-text">You have no games yet. Create one with the "Create New Game" button!</div>');
        }

        let currentView = '@viewParam';
        function getEndpointForView(viewId) {
            switch (viewId) {
                case '9': return '/develop/asset-list/9'; // Places
                case '10': return '/develop/asset-list/10'; // Models
                case '13': return '/develop/asset-list/13'; // Decals
                case '21': return '/develop/asset-list/21'; // Badges
                case '3': return '/develop/asset-list/3'; // Audio
                case '24': return '/develop/asset-list/24'; // Animations
                case '40': return '/develop/asset-list/40'; // Meshes
                case '11': return '/develop/asset-list/11'; // Shirts
                case '2': return '/develop/asset-list/2'; // T-Shirts
                case '12': return '/develop/asset-list/12'; // Pants
                case '38': return '/develop/asset-list/38'; // Plugins
                default: return '/develop/games-list'; // Games (default)
            }
        }

        function loadMyItems() {
            var $itemsContainer = $('#MyCreationsTab .items-container');

            // Show loading spinner
            var $loading = $('<div class=\'buildpage-loading-container\'><img alt=\'Loading...\' src=\'https://images.rbxcdn.com/ec4e85b0c4396cf753a06fade0a8d8af.gif\'></div>');
            $itemsContainer.empty().append($loading);

            $.ajax({
                url: getEndpointForView(currentView),
                dataType: 'html'
            }).done(function (data) {
                $loading.remove();
                if (data && $.trim(data).length) {
                    $itemsContainer.html(data);
                    // Initialize thumbnail retries if helper exists
                    if ($.fn.loadRobloxThumbnails) {
                        $itemsContainer.find('a[data-retry-url]').loadRobloxThumbnails();
                    }
                } else {
                    showEmptyMessage($itemsContainer);
                }
            }).fail(function (xhr) {
                $loading.remove();
                if (xhr.status === 404) {
                    showEmptyMessage($itemsContainer);
                } else {
                    $itemsContainer.text('Failed to load games. Please try again later.');
                }
            });
        }

        function probeGroupCreations() {
            // Lightweight HEAD/GET just to see if endpoint exists
            $.ajax({
                url: '/develop/groups/games-list',
                type: 'GET',
                dataType: 'html'
            }).done(function (data) {
                if (data && $.trim(data).length) {
                    // API is available and returned content; show the tab and content area
                    $('#GroupCreationsTabLink').show();
                    $('#GroupCreationsTab').show();
                }
            });
        }

        loadMyItems();

        // Intercept tab clicks within My Creations menu so we can AJAX-load
        $('#MyCreationsTab .menu-area').on('click', 'a.tab-item', function (evt) {
            if (!/view=\d+/i.test($(this).attr('href'))) {
                return; // let default navigation occur for non-view tabs
            }
            const href = $(this).attr('href') || '';
            const match = href.match(/view=(\d+)/i);
            currentView = match ? match[1] : '';

            // Visually update selected tab
            $(this).siblings('.tab-item').removeClass('tab-item-selected');
            $(this).addClass('tab-item-selected');

            // Update header title
            const headerText = $(this).text();
            $('#MyCreationsTab .header-text').text(headerText);

            // Load new items for selected view
            loadMyItems();

            // Prevent full page reload
            evt.preventDefault();
        });
        probeGroupCreations();
    });
</script>
