                <script type="text/javascript">
                    (function () {
                        function updateSellMath() {
                            var priceInput = document.getElementById('ctl00_cphRoblox_PriceTextBox');
                            if (!priceInput) {
                                return;
                            }
                            var raw = priceInput.value || '';
                            var numeric = raw.replace(/[^0-9]/g, '');
                            var price = parseInt(numeric, 10);
                            if (isNaN(price) || price < 0) {
                                price = 0;
                            }
                            var amountSpans = document.querySelectorAll('#SellItem .sell-robux-amount');
                            if (!amountSpans || amountSpans.length < 2) {
                                return;
                            }
                            var fee = 0;
                            var earn = 0;
                            if (price > 0) {
                                fee = Math.floor(price * 0.3);
                                if (fee < 1) {
                                    fee = 1;
                                }
                                earn = price - fee;
                                if (earn < 0) {
                                    earn = 0;
                                }
                            }
                            amountSpans[0].innerHTML = fee;
                            amountSpans[1].innerHTML = earn;
                        }
                        function updateSellPricingVisibility() {
                            var cb = document.getElementById('ctl00_cphRoblox_SellThisItemCheckBox');
                            var panel = document.getElementById('SellItemPricing');
                            if (!cb || !panel) {
                                return;
                            }
                            if (cb.checked) {
                                panel.style.display = '';
                                updateSellMath();
                            } else {
                                panel.style.display = 'none';
                            }
                        }
                        window.updateSellMath = updateSellMath;
                        var priceInput = document.getElementById('ctl00_cphRoblox_PriceTextBox');
                        if (priceInput) {
                            priceInput.onkeyup = updateSellMath;
                            priceInput.onchange = updateSellMath;
                        }
                        var sellCb = document.getElementById('ctl00_cphRoblox_SellThisItemCheckBox');
                        if (sellCb) {
                            sellCb.onclick = updateSellPricingVisibility;
                        }
                        updateSellPricingVisibility();
                    })();
                </script>
