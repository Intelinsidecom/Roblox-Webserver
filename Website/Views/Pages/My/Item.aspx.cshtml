@using Assets
@using Microsoft.Extensions.Configuration
@inject IConfiguration Configuration
@{ Layout = "_Layout"; }
@{
    var idRaw = Context.Request.Query["id"].ToString();
    if (!long.TryParse(idRaw, out var itemId) || itemId <= 0)
    {
        Context.Response.Redirect("/404");
        return;
    }

    var connStr = Configuration.GetConnectionString("Default");
    if (string.IsNullOrWhiteSpace(connStr))
    {
        Context.Response.Redirect("/404");
        return;
    }

    var resolvedId = AssetHelpers.ResolveLinkedAssetId(connStr, itemId);

    var isPost = string.Equals(Context.Request.Method, "POST", StringComparison.OrdinalIgnoreCase);
    var action = isPost ? Context.Request.Form["Action"].ToString() : string.Empty;
    var updateSucceeded = false;
    DateTime? updateTime = null;

    if (isPost && string.Equals(action, "Cancel", StringComparison.OrdinalIgnoreCase))
    {
        Context.Response.Redirect($"/catalog/{itemId}/haaa");
        return;
    }

    if (isPost && string.Equals(action, "Save", StringComparison.OrdinalIgnoreCase))
    {
        var postedName = Context.Request.Form["ctl00$cphRoblox$NameTextBox"].ToString();
        var postedDescription = Context.Request.Form["ctl00$cphRoblox$DescriptionTextBox"].ToString();
        var sellCheckboxRaw = Context.Request.Form["ctl00$cphRoblox$SellThisItemCheckBox"].ToString();
        var onSalePosted = !string.IsNullOrWhiteSpace(sellCheckboxRaw);
        var priceRaw = Context.Request.Form["ctl00$cphRoblox$RobuxPrice"].ToString();
        var allowCommentsRaw = Context.Request.Form["ctl00$cphRoblox$EnableCommentsCheckBox"].ToString();
        var allowCommentsPosted = !string.IsNullOrWhiteSpace(allowCommentsRaw);
        var genreRaw = Context.Request.Form["ctl00$cphRoblox$actualGenreSelection"].ToString();

        int parsedGenre = 1;
        if (!string.IsNullOrWhiteSpace(genreRaw) && int.TryParse(genreRaw, out var tmpGenre) && tmpGenre >= 1 && tmpGenre <= 15)
        {
            parsedGenre = tmpGenre;
        }

        long parsedPrice = 0;
        if (!string.IsNullOrWhiteSpace(priceRaw))
        {
            var digitsOnly = new string(priceRaw.Where(char.IsDigit).ToArray());
            if (!string.IsNullOrWhiteSpace(digitsOnly) && long.TryParse(digitsOnly, out var tmp) && tmp >= 0)
            {
                parsedPrice = tmp;
            }
        }

        if (!onSalePosted)
        {
            parsedPrice = 0;
        }

        var assetsRepo = new AssetsRepository();
        try
        {
            if (!string.IsNullOrWhiteSpace(postedName))
            {
                assetsRepo.UpdateAssetMetadataAsync(connStr, resolvedId, postedName, postedDescription)
                    .GetAwaiter().GetResult();
            }

            assetsRepo.UpdateAssetSaleAsync(connStr, resolvedId, onSalePosted, parsedPrice)
                .GetAwaiter().GetResult();

            assetsRepo.UpdateAssetAllowCommentsAsync(connStr, resolvedId, allowCommentsPosted)
                .GetAwaiter().GetResult();

            assetsRepo.UpdateAssetGenreAsync(connStr, resolvedId, parsedGenre)
                .GetAwaiter().GetResult();

            updateSucceeded = true;
            updateTime = DateTime.Now;
        }
        catch
        {
        }
    }

    var repo = new AssetMetadataRepository();
    AssetRecord record;
    try
    {
        record = repo.GetAssetByIdAsync(connStr, resolvedId).GetAwaiter().GetResult();
    }
    catch
    {
        Context.Response.Redirect("/404");
        return;
    }

    if (record == null)
    {
        Context.Response.Redirect("/404");
        return;
    }

    var assetName = record.Name ?? string.Empty;
    var assetTypeLabel = AssetTypeNames.GetConfigureLabel(record.AssetTypeId);
    var assetDescription = record.Description ?? string.Empty;
    var primaryThumb = string.IsNullOrWhiteSpace(record.HighResThumbnailUrl)
        ? record.ThumbnailUrl
        : record.HighResThumbnailUrl;
    var assetThumbnail = string.IsNullOrWhiteSpace(primaryThumb)
        ? "/images/RobloxLogo.png"
        : primaryThumb;
    var onSale = record.OnSale;
    var assetPrice = record.Price;
    var allowComments = record.AllowComments;
    var assetGenre = record.Genre;
    var minPrice = 2;
}

