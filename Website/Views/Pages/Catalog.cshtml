<script type="text/javascript">
    $(function () {
        function shouldStayEnabled($link) {
            var text = $.trim($link.text());
            return text === "All Featured Items" || text === "T-Shirts";
        }

        // Disable all assetTypeFilter / gearFilter / breadCrumbFilter tabs except the allowed ones
        $("a.assetTypeFilter, a.gearFilter, a.breadCrumbFilter").each(function () {
            var $link = $(this);
            if (!shouldStayEnabled($link)) {
                $link.addClass("disabled-tab");
                $link.on("click", function (e) {
                    e.preventDefault();
                    return false;
                });
            }
        });
    });
</script>

